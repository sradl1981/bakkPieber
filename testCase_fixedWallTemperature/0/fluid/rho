FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type             fixedValue;
        value            uniform 1;
    }
    
    outlet
    {
        type             fixedValue;
        value            uniform 1;
    }
    
    fluid_to_bedOfSpheres
    {
        type             fixedValue;
        value            uniform 1;
    }

    tubeWall
    {
        type             fixedValue;
        value            uniform 1;
    }  
}   

