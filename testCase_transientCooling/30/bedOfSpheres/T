FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "30/bedOfSpheres";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 400;

boundaryField
{
    bedOfSpheres_to_fluid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            T;
        kappa           solidThermo;
        kappaName       none;
        value           uniform 400;
    }
}


